

@{
    ViewBag.Title = "ViewDetails";

    var a = new WebGrid(Model);
    a.Pager(WebGridPagerModes.All);

}

<h2>ViewDetails</h2>
<style type="text/css">
  .MyTable {
   font-family: Tahoma, Arial, Helvetica, sans-serif;
   font-size: 11px;
   color: #000000;
   height: 60px;
   width: 1150px;
   border: 1px solid Black;
   }
  </style>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Branch Master</title>
    <link href="@Url.Content("~/Content/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/tabPanel.css")" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/js/myjquery.js"></script>
    <script src="@Url.Content("~/Scripts/jquery-2.1.4.min.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Scripts/js/myjquery.js")" type="text/javascript" ></script>
</head>
<body>
   @* @foreach (var i in ViewBag.L)
    {   
        @i.
        
    }*@
    @foreach (var item in Model)
    {
         
        <tr>
            <td>@Html.DisplayFor(modelItem => item.ID)</td>
            <td>@Html.DisplayFor(modelItem => item.NAME)</td>
            <td>@Html.DisplayFor(modelItem => item.CATEGORY)</td>
            <td>@Html.DisplayFor(modelItem => item.BranchName)</td>
          <td>@Html.DisplayFor(modelItem => item.CreateDate)</td>
            @*<td style="padding-left: 15px">
                @Html.ActionLink("Student", "Create", "Student", item, null) |
                @Html.ActionLink("Instructor", "Create", "Instructor", item, null)
            </td>*@
        </tr>
    
    }
    <div class="container">
   
  <TABLE id="Table1" class="MyTable">
            <tr>
            <td><label id="lblIN" class="filter-col control-label">Institute Name :</label></td>
              <td> <input id="txtIN" class="form-control input-sm" type="text" value="@item"/></td><td> </td>
              <td><label id="lblCat" class="filter-col control-label">Category Name :</label></td>
              <td> <input id="txtCat" class="form-control input-sm" type="text" value=""/></td><td> </td>
             <td><label id="lblBName" class="filter-col control-label">Branch Name :</label></td>
              <td> <input id="txtBName" class="form-control input-sm" type="text" /></td><td> </td>
             <td><label id="lblCDate" class="filter-col control-label">Create date :</label></td>
              <td> <input id="txtCDate" class="form-control input-sm" type="text" /></td>
               </tr>                                                                                          
            </table> 
 
           <div class="row">                        
                <div class="form-group">                                                     
            </div>      
        </div>
        <div class="row">
            <div class="col-xs-6">
                <div class="form-group">
                    <label id="lblAdd1" class="filter-col control-label">Address 1 :</label>
                    <input id="txtAdd1" class="form-control input-sm" type="text" />
                </div>
            </div>
            <div class="col-xs-6">
                <div class="form-group">
                    <label id="lblAdd2" class="filter-col control-label">Address 2 :</label>
                    <input id="txtAdd2" class="form-control input-sm" type="text" />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-6">
                <div class="form-group">
                    <label id="lblAdd3" class="filter-col control-label">Address 3 :</label>
                     <input id="txtAdd3" class="form-control input-sm" type="text" />
                </div>
            </div>
            <div class="col-xs-6">
            </div>
        </div>

        <div class="row">
            <div class="col-xs-6">
                <div class="form-group">
                    <label id="lblCountry" class="filter-col control-label">Country :</label>
                    <select id="selCountry" class="dropdown"></select>
                    @*@Html.DropDownListFor(model => model.Country.Id, new SelectList(Model.country, "Value", "Text"),  new { @id = "Country" }                                                           
                    )*@
                </div>
            </div>
            <div class="col-xs-6">
                <div class="form-group">
                    <label id="lblState" class="filter-col control-label">State :</label>
                    <select id="selState" style="color: black"></select>
                    @* @Html.DropDownListFor(model=>model.State.Id,new SelectList(Model.state,"Value","Text"), new {@id="State"})*@
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-xs-6">
                <div class="form-group">
                    <label id="lblCity" class="filter-col control-label">City :</label>
                    <input id="txtCity" class="form-control input-sm" type="text" />
                </div>
            </div>
            <div class="col-xs-6">
                <div class="form-group">
                    <label id="lblPincode" class="filter-col control-label">Pincode :</label>
                    <input id="txtPincode" class="form-control input-sm" type="text" />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-6">
                <label id="lblPhoneNo" class="filter-col control-label">Contact No :</label>
            </div>
            <div class="col-xs-6">
                <label id="lblEmailId" class="filter-col control-label">Financial Institute Email Id :</label>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-1">
                <div class="form-group">
                    <input id="txtContryCode" class="form-control input-sm" type="text" />
                </div>
            </div>
            <div class="col-xs-5">
                <div class="form-group">
                    <input id="txtContactNo" class="form-control input-sm" type="text" />
                </div>
            </div>

            <div class="col-xs-6">
                <div class="form-group">
                    <input id="txtEmailId" class="form-control input-sm" type="text" />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12">
                <div class="form-group">
                    <label id="lblContPersonDetails" class="filter-col control-label">Contact Person's Details :</label>
                    <textarea id="txtContPersonDetails" class="form-control input-sm"></textarea>
                </div>
            </div>

        </div>


        <div class="row">
            <div class="col-xs-6">
                <div class="form-group">
                    <label id="lblPerson1Contact" class="filter-col control-label">Person 1 Contact :</label>
                    <input id="txtPerson1Contact" class="form-control input-sm" type="text" />
                </div>
            </div>
            <div class="col-xs-6">
                <div class="form-group">
                    <label id="lblPerson2Contact" class="filter-col control-label">Person 2 Contact :</label>
                    <input id="txtPerson2Contact" class="form-control input-sm" type="text" />
                </div>
            </div>
        </div>

         <div class="row">
            <div class="col-xs-6">
                <div class="form-group">
                    <label id="lblPerson1Name" class="filter-col control-label">Person 1 Contact Mobile :</label>
                    <input id="txtPerson1name" class="form-control input-sm" type="text" />
                </div>
            </div>
            <div class="col-xs-6">
                <div class="form-group">
                    <label id="lblPerson2Name" class="filter-col control-label">Person 2 Contact Mobile :</label>
                    <input id="txtPerson2name" class="form-control input-sm" type="text" />
                </div>
            </div>
        </div>



        <div class="row">
            <div class="col-xs-6">
                <div class="form-group">
                    <label id="lblPerson1Email" class="filter-col control-label">Person 1 Email ID :</label>
                    <input id="txtPerson1Email" class="form-control input-sm" type="text" />
                </div>
            </div>
            <div class="col-xs-6">
                <div class="form-group">
                    <label id="lblPerson2Email" class="filter-col control-label">Person 2 Email ID :</label>
                    <input id="txtPerson2Email" class="form-control input-sm" type="text" />
                </div>
            </div>
        </div>



    </div>
    <div class="row">
        <div class="col-xs-5">
        </div>
        <div class="col-xs-1">
            <div class="form-group">
                <button id="btnAdd" type="button" name="ADD">ADD</button>
            </div>
        </div>
        <div class="col-xs-6">
            <button id="btnClear" type="button" name="RESET">RESET</button>
        </div>
    </div>
    
    } 
   
</body>
    </html>